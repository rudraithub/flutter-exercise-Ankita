0306

void main()
{
  int number1=10;
  int number2=20;
  int temp;
  temp=number1;
  number1=number2;
  number2=temp;
 print("number1 ${number1}");
   print("number2 ${number2}");
}
output:
number1 20
number2 10
--------------------------------------------------------------
0307

void main()
{
  int number1=10;
  int number2=20;
 number1=number1+number2;
  number2=number1-number2;
  number1=number1-number2;
  print("number1=$number1");
  print("number2=$number2");
}
output:
number1=20
number2=10

