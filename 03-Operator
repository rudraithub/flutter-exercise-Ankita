0303

void main()
{
  int a=52;
  int b=45;
  a+=b;
  print("add and assign : ${a}");
  a-=b;
  print("substract and assign : ${a}");
  a*=b;
  print("Multiplication and assign : ${a}");
  a~/=b;
  print("Devision and assign : ${a}");
  a%=b;
  print("modulo and assign : ${a}");
 }
output:
add and assign : 97
substract and assign : 52
Multiplication and assign : 2340
Devision and assign : 52
modulo and assign : 7
------------------------------------------------------------
0304

void main()
{
  int a=99;
  int b=12;
  int c;
  c=a+b;
  print("Add:value of c is ${c}");
  c=a-b;
  print("Sub:value of c is ${c}");
  c=a*b;
  print("Mul:value of c is ${c}");
  c=a~/b;
  print("Div:value of c is ${c}");
  c=a%b;
  print("Mod:value of c is ${c}");
}
output:
Add:value of c is 111
Sub:value of c is 87
Mul:value of c is 1188
Div:value of c is 8
Mod:value of c is 3
