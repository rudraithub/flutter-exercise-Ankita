void main()
{
 int num1=55;
 if(num1%5==0 && num1%11==0)
 {
   print("number is divisable by both 5 and 11");
 }
  else
  {
    print("number is not divisable by 5 and 11");
  }
}
output:
number is divisable by both 5 and 11
